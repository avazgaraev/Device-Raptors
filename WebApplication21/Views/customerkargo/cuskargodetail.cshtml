                                        <i class="fas fa-heart mr-15" aria-hidden="true"></i>
                                        Wishlist
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link
                                                " href="~/customerchgpass/Index" aria-controls="dashboard" aria-selected="false">
                                        <i class="fa fa-key mr-15" aria-hidden="true"></i>
                                        Change password
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link
                                                " href="~/logout/Index" aria-controls="dashboard" aria-selected="false">
                                        <i class="fa fa-sign-out-alt mr-15" aria-hidden="true"></i>
                                        Logout
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-8">
                        <div class="tab-content dashboard-content">
                            <div class="tab-pane fade active show" id="dashboard" role="tabpanel" aria-labelledby="dashboard-tab">
                                <div class="card">
                                    <div class="card-header">
                                        <h5 class="mb-0">Order detail #@Html.DisplayFor(x => x.kargono)</h5>
                                    </div>
                                    <div class="card-body">
                                        <div class="customer-order-detail">
                                            <div class="row">
                                                <div class="col-auto me-auto">
                                                    <div class="order-slogan">
                                                        <img width="100" src="~/Content/storage/general/logom.png" alt="Wowy - Laravel Multipurpose eCommerce Script">
                                                        <br>
                                                    </div>
                                                </div>
                                                <div class="col-auto">
                                                    <div class="order-meta">
                                                        <span class="d-inline-block">Time:</span>
                                                        <strong class="order-detail-value">@Html.DisplayFor(x => x.kargofoldate)</strong>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-12 border-top pt-2">
                                                    <h4>Order information</h4>
                                                    <div>
                                                        <div>
                                                            <span class="d-inline-block">Order status:</span>
                                                            <strong class="order-detail-value">@Html.DisplayFor(x => x.kargodetail)</strong>
                                                        </div>

                                                        @Html.Action("partialprod", "customerkargo")
                                                    </div>
                                                    @Html.Action("partialcus", "customerkargo")
                                                    <h4 class="mt-3 mb-1">Products</h4>
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-hover">
                                                            <thead>
                                                                <tr>
                                                                    <th>#</th>
                                                                    <th>Image</th>
                                                                    <th>Product</th>
                                                                    <th>Amount</th>
                                                                    <th style="width: 100px">Quantity</th>
                                                                    <th class="price text-right">Total</th>
                                                                </tr>
                                                            </thead>

                                                            <tbody>
                                                                @Html.Action("partialsale", "customerkargo")

                                                            </tbody>
                                                        </table>
                                                    </div>
                                                    <div class="mt-2 row">
                                                        <div class="col-auto me-auto">
                                                            <a href="#" class="btn btn-info btn-sm"><i class="fa fa-download"></i> Download invoice</a>
                                                        </div>
                                                        <div class="col-auto">
                                                            <a href="/customerkargo/kargodel/@Model.kargono" onclick="if (!window.__cfRLUnblockHandlers) return false; return confirm('Are you sure?')" class="btn btn-danger btn-sm ml-2">Cancel order</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</section>
